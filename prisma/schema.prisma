datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "go run github.com/steebchen/prisma-client-go"
  output   = "../internal/infrastructure/repository/prisma"
}

model Calculation {
  id        String   @id @default(cuid())
  operation String
  a         Int
  b         Int
  result    Int
  createdAt DateTime @default(now())
}
